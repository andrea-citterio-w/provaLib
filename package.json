{
    "name": "scratch-card-lib",
    "version": "1.0.0",
    "description": "Libreria per gratta e vinci digitale utilizzabile in Angular",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
      "dist/**/*",
      "angular/**/*"
    ],
    "scripts": {
      "clean": "rimraf dist",
