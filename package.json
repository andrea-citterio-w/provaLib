    "keywords": [
      "scratch-card",
      "angular",
      "typescript"
    ],
    "author": "",
    "license": "MIT",
    "devDependencies": {
      "@angular/common": "^15.0.0",
      "@angular/core": "^15.0.0",
      "copyfiles": "^2.4.1",
      "rimraf": "^4.1.2",
      "typescript": "~4.9.5"
    },
    "peerDependencies": {
      "@angular/common": ">=12.0.0",
      "@angular/core": ">=12.0.0"
    }
  }